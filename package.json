{
  "name": "react-router-drilldown",
  "version": "0.0.0-development",
  "description": "provides drilldown-style horizontal slide transitions between index and child routes",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint demo.webpack.config.js src test stories",
    "lint:fix": "eslint --fix demo.webpack.config.js src test stories",
    "lint:watch": "esw --watch demo.webpack.config.js src test stories",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore lib/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "gen-flow-files": "flow gen-flow-files src/ --out-dir lib",
    "copy-flow-files": "cd src; copy *.js.flow **/*.js.flow ../lib",
    "build": "rimraf lib && babel src --out-dir lib",
    "test": "NODE_ENV=production BABEL_ENV=test nyc --reporter=lcov --reporter=text mocha $npm_package_config_mocha",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run lint && flow",
    "prepush": "npm test",
    "prepublish": "npm run lint && flow && npm test && npm run build && npm run copy-flow-files",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:demo": "webpack --config demo.webpack.config.js --progress",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "mocha": "--compilers js:babel-core/register -r jsdom-global/register ./test/**.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/react-router-drilldown.git"
  },
  "keywords": [
    "react",
    "react-router",
    "drilldown",
    "route",
    "transition",
    "animation"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/react-router-drilldown/issues"
  },
  "homepage": "https://github.com/jcoreio/react-router-drilldown#readme",
  "devDependencies": {
    "@jedwards1211/eslint-config-flow": "^1.0.0",
    "@jedwards1211/eslint-config-react": "^1.0.1",
    "@kadira/storybook": "^2.35.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-flow-react-proptypes": "^2.1.3",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "copy": "^0.3.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.8.2",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "flow-bin": "^0.37.4",
    "flow-watch": "^1.1.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "lodash.size": "^4.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.5.4",
    "react-transition-context": "^1.0.0-beta8",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "sinon": "^1.17.6",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0",
    "react-router": "^2.0.0 || ^3.0.0"
  },
  "optionalDependencies": {
    "react-transition-context": "^1.0.0-beta8"
  },
  "dependencies": {
    "inline-style-prefixer": "^2.0.5"
  }
}
